<idea-plugin>
	<id>org.mustbe.consulo.aspnet</id>
	<name>ASP .NET 4</name>
	<version>SNAPSHOT</version>
	<vendor>Must-Be.org</vendor>
	<category>Web Frameworks</category>
	<depends>org.mustbe.consulo.dotnet.core</depends>
	<depends config-file="microsoft-dotnet.xml" optional="true">org.mustbe.consulo.dotnet.microsoft</depends>
	<depends config-file="mono-dotnet.xml" optional="true">org.mustbe.consulo.dotnet.mono</depends>

	<extensions defaultExtensionNs="com.intellij">
		<sdkType implementation="org.mustbe.consulo.aspnet.bundle.IISExpressBundleType" />

		<configurationType implementation="org.mustbe.consulo.aspnet.module.run.AspNetConfigurationType" />
	</extensions>
</idea-plugin>